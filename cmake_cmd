cmake -Wno-dev -DCMAKE_VERBOSE_MAKEFILE:BOOL=ON -DCMAKE_RULE_MESSAGES=ON -DCMAKE_BUILD_TYPE=Release \
 -DTRANSLATIONS=EN \
 -DPCB=X10 -DPCBREV=TX16S \
 -DDEFAULT_MODE=2 -DPPM_UNIT=US \
 -DGVARS=YES -DLUA=YES \
 -DHELI=NO -DTIMERS=2 -DFLIGHT_MODES=OFF \
 -DINTERNAL_MODULE_CRSF=OFF -DINTERNAL_GPS=NO -DUSB_SERIAL=OFF \
 ../
